<script type="text/javascript" src = "/js/delete.js"></script>
<h1>Hey, <%= @user.username %></h1>
<div class= "profile_created">
  <h2>Surveys Taken</h2>
  <strong><a href="/browse">Browse Surveys</a></strong>
  <% @answered_surveys.each do |survey| %>
    <li><a href="/survey/<%= survey.survey_id %>/results"><%= survey.title %></a></li>
  <% end %>

</div>
<aside>
  <h2>Created Surveys</h2>   
  <strong><a href="/survey/create">Create New Survey</a></strong>
  <% @created_surveys.each do |survey| %>
    <li>
        <a id="surveyid" href="/survey/<%= survey.id %>/results"><%= survey.title %></a>
        <button class="delete">DELETE</button>
    </li>
  <% end %>
</aside> 

